{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/book-shelf/index",
      "style": {
        "navigationBarTitleText": "书架"
      }
    },
    {
      "path": "pages/book-market/index",
      "style": {
        "navigationBarTitleText": "书城"
      }
    },
    {
      "path": "pages/book-store/index",
      "style": {
        "navigationBarTitleText": "书库"
      }
    },
    {
      "path": "pages/user/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "阅读小程序",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#f9f9f9"
  },
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/book-shelf/index"
      },
      {
        "pagePath": "pages/book-market/index"
      },
      {
        "pagePath": "pages/book-store/index"
      },
      {
        "pagePath": "pages/user/index"
      }
    ]
  }
}
